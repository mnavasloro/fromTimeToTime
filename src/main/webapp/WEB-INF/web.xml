<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>annotateDoc</servlet-name>
        <servlet-class>oeg.core.tagger.servlets.annotateDoc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>qlegalwhen</servlet-name>
        <servlet-class>oeg.core.tagger.servlets.queryLW</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>annotateDoc</servlet-name>
        <url-pattern>/annotateDoc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>qlegalwhen</servlet-name>
        <url-pattern>/qlegalwhen</url-pattern>
    </servlet-mapping>
</web-app>
